{
    "env": {
        "commonjs": true,
        "es2021": true,
        
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "linebreak-style": "off",
        "max-len": "off",
        "no-unused-vars": "off",
        "no-var": "off",
        "quotes": "off",
        "comma-dangle": "off",
        "operator-linebreak":"off",
        "object-curly-newline":"off"
    }
}
